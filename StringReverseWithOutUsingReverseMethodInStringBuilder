public class StringReverseWithOutUsingReverseMethodInStringBuilder {

	public static void main(String[] args) {
		String givenName = "Raja";
		char[] ch = givenName.toCharArray();
		String reverseName = "";
		for(int i = givenName.length()-1;i>=0;i--) {
			reverseName = reverseName+givenName.charAt(i);
		}
		 System.out.println("Reversed string is:");
	        System.out.println(reverseName);
	}

}
